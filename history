;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(extended-command-history))
(setq extended-command-history '("help-mode"))
(setq kill-ring 'nil)
(setq command-history '((help-mode) (execute-extended-command nil "help-mode" "help-mo")))
(setq set-variable-value-history 'nil)
(setq custom-variable-history 'nil)
(setq query-replace-history 'nil)
(setq read-expression-history 'nil)
(setq minibuffer-history 'nil)
(setq read-char-history 'nil)
(setq face-name-history 'nil)
(setq file-name-history 'nil)
